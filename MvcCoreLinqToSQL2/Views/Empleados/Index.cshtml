@model List<Empleado>

<h1>Linq Empleados</h1>
<table>
	<thead>
		<tr>
			<th>Apellido</th>
			<th>Oficio</th>
			<th>Salario</th>
		</tr>
	</thead>
	<tbody>
		@foreach (Empleado empleado in Model)
		{
			<tr>
				<td>@empleado.Apellido</td>
				<td>@empleado.Oficio</td>
				<td>@empleado.Salario</td>
				<td>
					<a asp-controller="Empleados" asp-action="Details" asp-route-idempleado="@empleado.IdEmpleado">Details</a>
				</td>
			</tr>
		}
	</tbody>
</table>